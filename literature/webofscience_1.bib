@article{ WOS:000271537400005,
Author = {Halin, Izhal Abdul and Hasan, Wan ZuhaWan and Shafie, Suhaidi},
Title = {Soil moisture sensor and read-out circuit topology for large array
   deployment},
Journal = {IEICE ELECTRONICS EXPRESS},
Year = {2009},
Volume = {6},
Number = {17},
Pages = {1234-1239},
Month = {SEP 10},
Abstract = {This paper focuses on the analysis of a BJT based soil moisture sensor
   that outputs a voltage corresponding to the change in soil resistance.
   The measured sensor's sensitivity is 51.1 mV/\% with a deviation of 19.7
   mV from sensor to sensor. The deviation which is more than twice smaller
   than the sensitivity suggests that it is suitable to be used in a large
   array to detect gravimetric moisture in individual soil samples.
   Integration of read-out circuitry to the sensor shows that an array
   operation of the sensor is possible and can be used to aid in moisture
   detection in fertigation farms.},
