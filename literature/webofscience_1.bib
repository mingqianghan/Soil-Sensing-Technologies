@article{ WOS:000328005300034,
Author = {Ma, Daokun and Li, Daoliang and Ding, Qisheng and Wang, Cong},
Title = {A Combined Sensor for Simultaneous Measurements of Soil Moisture and
   Electrical Conductivity},
Journal = {SENSOR LETTERS},
Year = {2013},
Volume = {11},
Number = {6-7, SI},
Pages = {1195-1198},
Month = {JUN-JUL},
Abstract = {A combined sensor for simultaneous measurements of soil water content
   and electrical conductivity in situ was designed, which integrated the
   frequency domain and soil resistance methods. A serial experiments were
   established in lab to evaluate the performance of the combined sensor.
   The calibration and tests in KCl solution and moist soil showed both
   outputs of the combined sensor had the different response styles to soil
   water content and electrical conductivity. The combined sensor will be a
   useful tool for measurement soil water content and electrical
   conductivity.},
