Journal = {ELECTRONICS AND COMMUNICATIONS IN JAPAN PART I-COMMUNICATIONS},
Year = {2007},
Volume = {90},
Number = {2},
Pages = {1-11},
Abstract = {To clarify the scattering characteristics of the ground surface for Ka
