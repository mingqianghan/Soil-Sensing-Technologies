}

@article{ WOS:001029698800001,
Author = {Shehata, Mahmoud and Gentine, Pierre and Nelson, Natalie and Sayde,
   Chadi},
Title = {Optimization of the number and locations of the calibration stations
   needed to monitor soil moisture using distributed temperature sensing
   systems: A proof-of-concept study},
Journal = {JOURNAL OF HYDROLOGY},
Year = {2023},
Volume = {620},
Number = {A},
Month = {MAY},
Abstract = {The single-probe heat-pulse (SPHP) technique combined with the
   Fiber-optic Distributed Temperature Sensing (DTS) technology can offer
   novel high-resolution measurements of soil moisture (\& theta;) over
   spatial scales ranging from several centimeters to several kilometers.
   However, the key limitation of this method is in obtaining the
   calibration relationship between \& theta; and soil thermal conductivity
   (\& lambda;) across a specific field. In a previous study, a new
   methodology using a Gaussian processes model was presented to account
   for the spatial variability in the \& lambda;-\& theta; relationship.
   The model aggregated \& theta; measurements from soil moisture sensors
   scattered over the SPHP transect with the corresponding DTS \& lambda;
   measurements at their locations. In this study, a novel methodology is
   tested to optimize the number and locations of soil moisture sensors
   required to account for the spatial variability of the \& lambda;-\&
   theta; relationship to achieve higher accuracy from the SPHP technique.
   The proposed methodology utilizes hierarchical clustering to analyze the
   information contained in the spatial structure of the SPHP measurements
   as the soil dries from a nearly-saturated condition. The proposed
   methodology was tested using data from a field in Oklahoma. Monte-Carlo
   simulation was performed to validate the performance of the proposed
   methodology. The predictions obtained from the proposed methodology
   resulted in \& theta; measurements accuracy comparable to those obtained
   from the 10\% best Monte-Carlo iterations of randomly assigned soil
   moisture locations. This study demonstrates that the proposed
   methodology is more efficient than the traditional practice of randomly
   spreading calibration soil moisture sensors along the SPHP transect.},
DOI = {10.1016/j.jhydrol.2023.129449},
EarlyAccessDate = {APR 2023},
Article-Number = {129449},
ISSN = {0022-1694},
EISSN = {1879-2707},
ResearcherID-Numbers = {Gentine, Pierre/C-1495-2013
   Nelson, Natalie/M-1768-2019
   Shehata, Mahmoud/W-8239-2018
