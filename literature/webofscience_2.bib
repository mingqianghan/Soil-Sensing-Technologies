@article{ WOS:000733170100001,
Author = {Akbar, Ruzbeh and Prager, Samuel and Silva, Agnelo R. and Moghaddam,
   Mahta and Entekhabi, Dara},
Title = {Wireless Sensor Network Informed UAV Path Planning for Soil Moisture
   Mapping},
Journal = {IEEE TRANSACTIONS ON GEOSCIENCE AND REMOTE SENSING},
Year = {2022},
Volume = {60},
Abstract = {Adaptive and targeted allocation of mobile sensing agents, in the form
   of unmanned aerial vehicles (UAVs) with software defined radar
   (UAV-SDRadar) payloads, enable mapping of surface soil moisture in
   regions where in situ wireless sensor networks (WSNs) undersample soil
   moisture or upscaling models perform poorly. This work presents an
   optimization-based UAV path planning methodology that seeks to maximize
   UAV flight coverage over areas where a complementing WSN yields upscaled
   soil moisture estimates with high uncertainty. By recursively mapping
   soil moisture over such areas, the combined UAV and WSN instrumentation
   can gradually capture the domain's true mean soil moisture. A series of
   numerical simulations are presented to demonstrate the algorithm's basic
   function while considering real-world and feasible operational
   scenarios.},
DOI = {10.1109/TGRS.2021.3088658},
