@article{ WOS:000916865200001,
Author = {Shakya, Amit Kumar and Ramola, Ayushman and Vidyarthi, Anurag},
Title = {Integrated modelling of soil moisture by evaluating backscattering
   models Dubois, Oh and IoT sensor development for field moisture
   estimation},
Journal = {MODELING EARTH SYSTEMS AND ENVIRONMENT},
Year = {2023},
Volume = {9},
Number = {3},
Pages = {3381-3402},
Month = {SEP},
Abstract = {This research work presents details about various soil moisture models,
   analysis of radar backscatter models Dubois and Oh using microwave
   polarization backscattering coefficients, i.e. crhh, crvv,sigma hv and
