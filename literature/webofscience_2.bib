   He, Huaiyu/C-3414-2018
   },
ORCID-Numbers = {xue, changbin/0000-0003-1834-7849
   Tang, Junyue/0000-0002-8825-3453},
Unique-ID = {WOS:001034752700001},
}

@article{ WOS:001136277700001,
Author = {Akash, M. and Kumar, P. Mohan and Bhaskar, Pradeep and Deepthi, P. R.
   and Sukhdev, Anu},
Title = {Review of estimation of soil moisture using active microwave remote
   sensing technique},
Journal = {REMOTE SENSING APPLICATIONS-SOCIETY AND ENVIRONMENT},
Year = {2024},
Volume = {33},
Month = {JAN},
Abstract = {The moisture level in the soil has significant consideration for
   applications in agriculture such as crop growth, yield percentage, land
   stability, and heat exchange. A thorough examination of these properties
   will provide explanations for hydrological, meteorological, and
   agricultural applications. Remote sensing techniques such as thermal,
   optical, active, and passive microwave measurements can precisely study
   the aforementioned properties. This paper examines current advances and
   prospective studies in agricultural applications using active remote
   sensing methods, concluding that remote sensing techniques have a
   greater potential for exploring many domains of agricultural
   applications.},
DOI = {10.1016/j.rsase.2023.101118},
