@article{ WOS:000533346400112,
Author = {Chen, Ming and Zhang, Miao and Wang, Xuming and Yang, Qingliang and
   Wang, Maohua and Liu, Gang and Yao, Lan},
Title = {An All-Solid-State Nitrate Ion-Selective Electrode with Nanohybrids
   Composite Films for In-Situ Soil Nutrient Monitoring},
Journal = {SENSORS},
Year = {2020},
Volume = {20},
Number = {8},
Month = {APR},
Abstract = {In this paper, an all-solid-state nitrate doped polypyrrole (PPy(NO3-)
   ion-selective electrode (ISE) was prepared with a nanohybrid composite
   film of gold nanoparticles (AuNPs) and electrochemically reduced
   graphene oxide (ERGO). Preliminary tests on the ISE based in-situ soil
   nitrate-nitrogen (NO3--N) monitoring was conducted in a laboratory
   3-stage column. Comparisons were made between the NO3--N content of
   in-situ soil percolate solution and laboratory-prepared extract
   solution. Possible influential factors of sample depth, NO3--N content,
   soil texture, and moisture were varied. Field-emission scanning electron
   microscopy (FESEM) and X-ray powder diffraction (XRD) characterized
   morphology and content information of the composite film of ERGO/AuNPs.
   Due to the performance excellence for conductivity, stability, and
   hydrophobicity, the ISE with ERGO/AuNPs illustrates an acceptable
   detection range from 10(-1) to 10(-5) M. The response time was
   determined to be about 10 s. The lifetime was 65 days, which revealed
   great potential for the implementation of the ERGO/AuNPs mediated ISE
   for in-situ NO3--N monitoring. In-situ NO3--N testing results conducted
   by the all-solid-state ISE followed a similar trend with the standard
   UV-VIS method.},
DOI = {10.3390/s20082270},
