@article{ WOS:000730789400169,
Author = {Wan, Wei and Ji, Rui and Liu, Baojian and Li, Huan and Zhu, Siyu},
Title = {A Two-Step Method to Calibrate CYGNSS-Derived Land Surface Reflectivity
   for Accurate Soil Moisture Estimations},
Journal = {IEEE GEOSCIENCE AND REMOTE SENSING LETTERS},
Year = {2022},
Volume = {19},
Abstract = {Based on a statistical analysis of the currently available 3-year
   on-orbit Cyclone Global Navigation Satellite System (CYGNSS) data
   (2017-2019), this study proposes a two-step calibration method to
   improve the accuracy of the CYGNSS-derived land surface reflectivity
   (SR) and the resulting soil moisture (SM) estimates. The method is
   designed for two purposes: the one is to correct the system errors of
   the SR estimates induced by the calibration of the CYGNSS Version 2.1
   L1B data, and the other is to eliminate vegetation attenuation in the SR
   of the soil layer. Mean SR corrections of similar to-0.9 and 2.2 dB are
   achieved through the first and second steps of calibration,
   respectively. This resulted in better SM estimates compared with the
   Soil Moisture Active Passive (SMAP) product and the in situ
   measurements, i.e., improved correlations between the CYGNSS SR and the
   SMAP SM (from R = 0.46 to R = 0.74), improved correlations between the
   CYGNSS SR and the in situ SM (from R = 0.47 to R = 0.62), and between
   the CYGNSS SM and the in situ SM with the best R = 0.64.},
DOI = {10.1109/LGRS.2020.3023650},
