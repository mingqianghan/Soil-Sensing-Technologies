@article{ WOS:000977272400001,
Author = {Nijaguna, G. S. and Manjunath, D. R. and Abouhawwash, Mohamed and Askar,
   S. S. and Basha, D. Khalandar and Sengupta, Jewel},
Title = {Deep Learning-Based Improved WCM Technique for Soil Moisture Retrieval
   with Satellite Images},
Journal = {REMOTE SENSING},
Year = {2023},
Volume = {15},
Number = {8},
Month = {APR},
Abstract = {The water cycle around the globe is significantly impacted by the
   moisture in the soil. However, finding a quick and practical model to
   cope with the enormous amount of data is a difficult issue for remote
   sensing practitioners. The traditional methods of measuring soil
   moisture are inefficient at large sizes, which can be replaced by remote
   sensing techniques for obtaining soil moisture. While determining the
   soil moisture, the low return frequency of satellites and the lack of
   images pose a severe challenge to the current remote sensing techniques.
   Therefore, this paper suggested a novel technique for Soil Moisture
   Retrieval. In the initial phase, image acquisition is made. Then, VI
   indexes (NDVI, GLAI, Green NDVI (GNDVI), and WDRVI features) are
   derived. Further, an improved Water Cloud Model (WCM) is deployed as a
   vegetation impact rectification scheme. Finally, soil moisture retrieval
   is determined by the hybrid model combining Deep Max Out Network (DMN)
   and Bidirectional Gated Recurrent Unit (Bi-GRU) schemes, whose outputs
   are then passed on to enhanced score level fusion that offers final
   results. According to the results, the RMSE of the Hybrid Classifier
   (Bi-GRU and DMN) method was lower (0.9565) than the RMSE of the Hybrid
   Classifier methods. The ME values of the HC (Bi-GRU and DMN) were also
   lower (0.728697) than those of the HC methods without the vegetation
   index, the HC methods without the presence of water clouds, and the HC
   methods with traditional water clouds. In comparison to HC (Bi-GRU and
   DMN), the HC method without vegetation index has a lower error of 0.8219
   than the HC method with standard water cloud and the HC method without
   water cloud.},
DOI = {10.3390/rs15082005},
Article-Number = {2005},
EISSN = {2072-4292},
ResearcherID-Numbers = {Askar, Sameh/ABA-6011-2020
